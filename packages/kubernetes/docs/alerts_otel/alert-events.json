POST kbn:/api/alerting/rule
{
  "params": {
    "criteria": [
      {
        "comparator": ">",
        "metrics": [
          {
            "name": "A",
            "aggType": "count"
          }
        ],
        "threshold": [
          20
        ],
        "timeSize": 1,
        "timeUnit": "m"
      }
    ],
    "alertOnNoData": false,
    "alertOnGroupDisappear": false,
    "searchConfiguration": {
      "query": {
        "query": "body_structured.object.kind : \"Event\"  and (body_structured.object.type : \"Warning\"  or body_structured.object.type : \"Error\") ",
        "language": "kuery"
      },
      "index": "logs-*"
    },
    "groupBy": [
      "body_structured.object.type"
    ]
  },
  "consumer": "logs",
  "schedule": {
    "interval": "5m"
  },
  "tags": [
    "Logs",
    "OTEL",
    "Events"
  ],
  "name": "Event Errors/ Warning ",
  "rule_type_id": "observability.rules.custom_threshold",
  "actions": []
}